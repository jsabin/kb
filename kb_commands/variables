export collector_restart="sudo /etc/init.d/event-collector restart"
export collector_running="ps -ef | grep collector"
export indexer_restart="sudo /etc/init.d/event-indexer restart"
export nagios_refresh_config="sudo /usr/local/nagios/bin/nagios -v /usr/local/nagios/etc/nagios.cfg"
export run_puppet="sudo /etc/runpuppet"
export nagios_config_log="cat /usr/local/nagios/var/cfgcheck.out"
export nagios_log="/usr/local/nagios/var/nagios.log"
export nagios_restart="sudo /etc/init.d/naigos restart"
export indexer_log="/opt/proofpoint/event-indexer/var/log/launcher.log"
